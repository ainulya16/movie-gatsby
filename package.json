  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start:dev": "gatsby develop -p 8000",
    "serve": "gatsby serve -p 8081",
    "start": "npm run start:dev",
    "test": "jest --ci --testPathIgnorePatterns .cache __mocks__ node_modules selenium_test __integration__ --updateSnapshot",
    "cover": "jest --ci --coverage --testPathIgnorePatterns .cache __mocks__ node_modules selenium_test __integration__",
    "test:custom": "jest --ci",
    "test:selenium": "npm run build && npm-run-all --parallel --race serve test:selenium:run",
    "test:selenium:run": "jest --ci --forceExit --detectOpenHandles selenium_test __integration__",
    "lint": "eslint \"**/*.js\" --fix",
    "e2e": "npm-run-all --parallel --race serve test",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "start:datasource": "babel-node src/dynamicForm/validator.js "
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
